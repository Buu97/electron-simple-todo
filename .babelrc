{
    "presets": [
        [
            "@babel/preset-env"
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        [
            "@babel/plugin-proposal-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ]
    ]
}